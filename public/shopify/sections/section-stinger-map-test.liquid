<!-- /sections/section-stinger-map.liquid-->
<style>
  .stinger-column-all{
    display: inline-block;
  }
  .stinger-column-all .stinger-column-outer{
    width:100%;
  }
  .stinger-column-all .stinger-column-inner{
    float:left;
    padding: 0.25rem 1rem;
    width:30%;
  }
@media only screen and (max-width: 1100px) {
  .stinger-column-all .stinger-column-inner{
    width:48% !important;
  }
}
  
</style>
<section>
  <div class="stinger-map-section">
  	<div class="stinger-map">
      <h2 class="mobile-show">Our Guide to Stingers</h2>
      {% render 'stinger-map' %}
    </div>
    <!-- desktop map -->
    <div class="">
      <h2 class="desktop-show" style="padding: 4rem 0 1rem 1rem;">Our Guide to Stingers</h2>
     <div class="stinger-column-all ">
        <div class="stinger-column-outer">
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>1</p></div><div class="stinger-type foreheadLines"><p>Forehead Lines</p></div>
          </div>
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>2</p></div><div class="stinger-type Glabella"><p>11s (Glabella)</p></div>
          </div>
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>3</p></div><div class="stinger-type BrowLift"><p>Brow Lift</p></div>
          </div>
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>4</p></div><div class="stinger-type CrowsFeet"><p>Crowâ€™s Feet</p></div>
          </div>
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>5</p></div><div class="stinger-type BunnyLines"><p>Bunny Lines</p></div>
          </div>
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>6</p></div><div class="stinger-type doa-6"><p>DAO</p></div>
          </div>
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>7</p></div><div class="stinger-type GummySmile"><p>Gummy Smile</p></div>
          </div>
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>8</p></div><div class="stinger-type LipFlip"><p>Lip Flip + Perioral</p></div>
          </div>
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>9</p></div><div class="stinger-type MentalisMuscle"><p>Mentalis Muscle</p></div>
          </div>
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>10</p></div><div class="stinger-type CosmeticMasseter"><p>Cosmetic Masseter</p></div>
          </div>
          <div class="stinger-column-inner">
            <div class="stinger-number"><p>11</p></div><div class="stinger-type stinger-neck"><p>Neck</p></div>
          </div>
        </div>
     </div>
      <div class="stinger-cta">
        <div class="double_card_header">
          <h4>{{ section.settings.stinger_header}}</h4>
        </div>
        <div class="double_card_body">
          <p>{{ section.settings.stinger_body }}</p>
        </div>
        <a role="button" href="https://formulafig.com/pages/appointments">
          <div><div class="book-now-locations">BOOK NOW</div></div>
          <!--       	  <div class="mobile-show"><div class="book-now-locations">BOOK CONSULTATION</div></div> -->
        </a>
      </div>
  </div><!-- end of desktop -->
  
        
</div><!-- end of map grid -->
    

</section>
{% schema %}
  {
    "name": "Stinger Map",
    "settings": [
		{
          "type": "text",
          "id": "stinger_header",
          "label": "Header Text"
        },{
          "type": "textarea",
          "id": "stinger_body",
          "label": "Subtitle Text"
        }
	],
  "presets": [
    {
    "name": "Stinger Map",
    "category": "ADVANCED LAYOUT"
    }
   ]
  }
{% endschema %}

